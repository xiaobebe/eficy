    "ava": "^2.1.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.0.6",
    "csstype": "^2.6.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "nyc": "^14.1.1",
